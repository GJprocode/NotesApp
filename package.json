    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.6.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.10"
